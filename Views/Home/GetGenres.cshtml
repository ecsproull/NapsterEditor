@model GenresViewModel

@{
    Layout = "_StartLayout";
    ViewBag.Title = "Genres";
    ViewBag.PageHeading = "Genres";
}

<table class="tracksTable">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Href</th>
        <th>Description</th>
    </tr>
    @foreach (var m in Model.Genres)
    {
        <tr>
            <td>@m.Id</td>
            <td>@m.Name</td>
            <td>@m.Href</td>
            <td>@m.Description</td>
        </tr>
    }
</table>